{
  "name": "chem",
  "version": "1.0.0",
  "description": "Chemistry Revision",
  "main": "preprocess.js",
  "scripts": {
    "build": "rm -r docs & node preprocess.js --minify",
    "dev": "concurrently --kill-others \"cd docs && live-server --no-browser\" \"when-changed -r src node preprocess.js --development\"",
    "preview": "cd docs && live-server",
    "deploy": "npm run build && node gh-pages.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veehz/chem.git"
  },
  "keywords": [
    "chemistry",
    "revision"
  ],
  "author": "veehz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/veehz/chem/issues"
  },
  "homepage": "https://github.com/veehz/chem#readme",
  "dependencies": {
    "html-minifier-terser": "^7.0.0",
    "katex": "^0.16.2",
    "preprocess": "^3.2.0"
  },
  "devDependencies": {
    "@compodoc/live-server": "^1.2.3",
    "concurrently": "^7.5.0",
    "gh-pages": "^4.0.0"
  }
}
